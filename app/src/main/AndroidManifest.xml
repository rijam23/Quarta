<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quarta">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/quartalogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/quartalogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quarta">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".LoanApplication" />
        <activity android:name=".ActiveLoans" />
        <activity android:name=".Profile" />
        <activity android:name=".QuarTalk" />
        <activity android:name=".HomeDashBoard" />
        <activity android:name=".Otp" />
        <activity android:name=".SignUp" />
        <activity android:name=".PaymentHistory">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>